.case-study {
  min-height: 100vh;
  background-color: #ffffff;
}

.navbar {
  display: flex;
  justify-content: center;
  padding: 30px 0;
  background-color: transparent;
  position: sticky;
  top: 0;
  z-index: 100;
}

.nav-container {
  display: flex;
  gap: 50px;
  background-color: #ffffff;
  padding: 18px 32px;
  border-radius: 30px;
  align-items: center;
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
  border: 1px solid rgba(0, 0, 0, 0.05);
  transition: box-shadow 0.3s ease, transform 0.3s ease;
}

.nav-container:hover {
  box-shadow: 0 6px 25px rgba(0, 0, 0, 0.12);
  transform: translateY(-2px);
}

.nav-link {
  text-decoration: none;
  color: #333333;
  font-weight: 500;
  font-size: 16px;
  transition: color 0.3s ease;
  padding: 8px 16px;
  border-radius: 20px;
}

.nav-link:hover {
  color: #666666;
  background-color: #f8f8f8;
}

.nav-link.active {
  color: #000000;
  font-weight: 600;
}

.get-in-touch {
  background-color: #000000;
  color: #ffffff !important;
  font-weight: 600;
}

.get-in-touch:hover {
  background-color: #333333 !important;
  color: #ffffff !important;
}

.case-study-hero {
  padding: 120px 20px;
  background-color: #ffffff;
  text-align: center;
}

.case-study-container {
  max-width: 800px;
  margin: 0 auto;
}

.n26-branding {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 16px;
  margin-bottom: 60px;
}

.n26-logo-section {
  display: flex;
  align-items: center;
  gap: 12px;
}

.n26-icon {
  display: flex;
  align-items: center;
  gap: 8px;
}

.n26-square {
  width: 32px;
  height: 32px;
  background-color: #20b2aa;
  border-radius: 8px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.n26-letter {
  color: #ffffff;
  font-size: 18px;
  font-weight: 700;
}

.n26-line {
  width: 24px;
  height: 3px;
  background-color: #20b2aa;
  border-radius: 2px;
}

.n26-text {
  font-size: 24px;
  font-weight: 700;
  color: #000000;
}

.n26-rating {
  display: flex;
  align-items: center;
  gap: 8px;
}

.stars {
  display: flex;
  gap: 2px;
}

.stars span {
  color: #ffd700;
  font-size: 16px;
}

.rating-number {
  font-size: 14px;
  font-weight: 600;
  color: #000000;
}

.case-study-title {
  font-size: 64px;
  font-weight: 800;
  line-height: 1.1;
  color: #000000;
  margin: 0 0 30px 0;
  text-align: center;
}

.case-study-subtitle {
  font-size: 20px;
  color: #666666;
  line-height: 1.5;
  margin: 0;
  text-align: center;
  max-width: 600px;
  margin-left: auto;
  margin-right: auto;
}

/* Phone Mockups Section */
.phone-mockups-section {
  padding: 120px 20px;
  background-color: #ffffff;
}

.phone-mockups-container {
  max-width: 1200px;
  margin: 0 auto;
}

.phone-mockups {
  display: flex;
  justify-content: center;
  align-items: flex-end;
  gap: 40px;
  position: relative;
}

.phone-left {
  transform: rotate(-5deg) translateY(20px);
  z-index: 1;
}

.phone-right {
  transform: rotate(5deg) translateY(-20px);
  z-index: 2;
}

.phone-mockup {
  width: 280px;
  height: 560px;
  background-color: #000000;
  border-radius: 40px;
  padding: 20px;
  position: relative;
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.2);
}

.phone-screen {
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  border-radius: 30px;
  overflow: hidden;
  position: relative;
}

/* Transaction Detail Screen (Left Phone) */
.transaction-detail {
  padding: 40px 30px;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.transaction-header {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 30px;
}

.merchant-logo {
  margin-bottom: 20px;
}

.airbnb-logo {
  width: 60px;
  height: 60px;
  background-color: #ff5a5f;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #ffffff;
  font-size: 24px;
  font-weight: 700;
}

.transaction-amount {
  font-size: 36px;
  font-weight: 700;
  color: #000000;
  text-align: center;
}

.transaction-info {
  text-align: center;
  margin-bottom: 40px;
}

.transaction-date {
  font-size: 16px;
  color: #666666;
}

.transaction-actions {
  display: flex;
  flex-direction: column;
  gap: 16px;
  margin-bottom: 40px;
}

.action-btn {
  display: flex;
  align-items: center;
  gap: 12px;
  padding: 16px 20px;
  border-radius: 12px;
  border: none;
  font-size: 16px;
  font-weight: 600;
  cursor: pointer;
  transition: all 0.3s ease;
}

.action-btn.green {
  background-color: #4caf50;
  color: #ffffff;
}

.action-btn.white {
  background-color: #ffffff;
  color: #000000;
  border: 1px solid #e0e0e0;
}

.action-icon {
  font-size: 20px;
}

.transaction-tags {
  margin-top: auto;
  text-align: center;
  font-size: 16px;
  color: #666666;
}

/* Home Screen (Right Phone) */
.home-screen {
  padding: 20px 20px 20px 20px;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.status-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20px;
  font-size: 14px;
  font-weight: 600;
}

.status-icons {
  display: flex;
  gap: 4px;
}

.home-header {
  text-align: center;
  margin-bottom: 30px;
}

.home-header h2 {
  font-size: 24px;
  font-weight: 700;
  color: #000000;
  margin: 0 0 16px 0;
}

.current-balance {
  font-size: 32px;
  font-weight: 700;
  color: #000000;
  margin-bottom: 8px;
}

.balance-label {
  font-size: 14px;
  color: #666666;
}

.transactions-section {
  flex: 1;
  overflow-y: auto;
}

.transaction-group {
  margin-bottom: 24px;
}

.group-title {
  font-size: 14px;
  font-weight: 600;
  color: #666666;
  margin-bottom: 12px;
  text-transform: uppercase;
  letter-spacing: 0.5px;
}

.transaction-item {
  display: flex;
  align-items: center;
  gap: 12px;
  padding: 12px 0;
  border-bottom: 1px solid #f0f0f0;
}

.transaction-item:last-child {
  border-bottom: none;
}

.transaction-icon {
  width: 40px;
  height: 40px;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 16px;
  font-weight: 700;
}

.spotify-logo {
  background-color: #1db954;
  color: #ffffff;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.airbnb-logo-small {
  background-color: #ff5a5f;
  color: #ffffff;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.restaurant-icon {
  background-color: #f5f5f5;
  color: #666666;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.transaction-details {
  flex: 1;
}

.merchant-name {
  font-size: 16px;
  font-weight: 600;
  color: #000000;
  margin-bottom: 4px;
}

.transaction-meta {
  font-size: 14px;
  color: #666666;
}

.transaction-amount {
  font-size: 16px;
  font-weight: 600;
  color: #000000;
}

/* Detailed Content Section */
.detailed-content-section {
  padding: 80px 0;
  background-color: #ffffff;
}

.content-layout {
  display: flex;
  max-width: 1400px;
  margin: 0 auto;
  padding: 0 40px;
  gap: 80px;
}

/* Sidebar Navigation */
.sidebar {
  width: 320px;
  flex-shrink: 0;
  position: sticky;
  top: 120px;
  height: fit-content;
}

.sidebar-nav {
  display: flex;
  flex-direction: column;
  gap: 4px;
}

.sidebar-item {
  display: flex;
  align-items: center;
  gap: 16px;
  padding: 16px 20px;
  border: none;
  background: none;
  text-align: left;
  cursor: pointer;
  transition: all 0.3s ease;
  border-radius: 8px;
  font-size: 16px;
  color: #999999;
  font-weight: 500;
}

.sidebar-item:hover {
  background-color: #f8f8f8;
  color: #333333;
}

.sidebar-item.active {
  background-color: #000000;
  color: #ffffff;
  font-weight: 700;
}

.sidebar-icon {
  font-size: 20px;
  width: 24px;
  text-align: center;
  color: #999999;
  font-family: 'Segoe UI Symbol', 'Arial Unicode MS', sans-serif;
}

.sidebar-item.active .sidebar-icon {
  color: #ffffff;
}

.sidebar-text {
  font-size: 16px;
  font-weight: inherit;
}

/* Icon-specific styles */
.sidebar-item:nth-child(1) .sidebar-icon::before { content: ''; }
.sidebar-item:nth-child(2) .sidebar-icon::before { content: '""'; }
.sidebar-item:nth-child(3) .sidebar-icon::before { content: ''; }
.sidebar-item:nth-child(4) .sidebar-icon::before { content: ''; }
.sidebar-item:nth-child(5) .sidebar-icon::before { content: ''; }
.sidebar-item:nth-child(6) .sidebar-icon::before { content: ''; }
.sidebar-item:nth-child(7) .sidebar-icon::before { content: ''; }
.sidebar-item:nth-child(8) .sidebar-icon::before { content: ''; }
.sidebar-item:nth-child(9) .sidebar-icon::before { content: ''; }
.sidebar-item:nth-child(10) .sidebar-icon::before { content: ''; }
.sidebar-item:nth-child(11) .sidebar-icon::before { content: ''; }

/* Alternative: Use Unicode symbols */
.sidebar-item[data-icon="book"] .sidebar-icon::before { content: ''; }
.sidebar-item[data-icon="quote"] .sidebar-icon::before { content: '""'; }
.sidebar-item[data-icon="target"] .sidebar-icon::before { content: ''; }
.sidebar-item[data-icon="id"] .sidebar-icon::before { content: ''; }
.sidebar-item[data-icon="chart"] .sidebar-icon::before { content: ''; }
.sidebar-item[data-icon="building"] .sidebar-icon::before { content: ''; }
.sidebar-item[data-icon="person"] .sidebar-icon::before { content: ''; }
.sidebar-item[data-icon="sparkle"] .sidebar-icon::before { content: ''; }
.sidebar-item[data-icon="graduation"] .sidebar-icon::before { content: ''; }
.sidebar-item[data-icon="megaphone"] .sidebar-icon::before { content: ''; }
.sidebar-item[data-icon="medal"] .sidebar-icon::before { content: ''; }

/* Main Content */
.main-content {
  flex: 1;
  max-width: 800px;
}

.content-section {
  margin-bottom: 120px;
  scroll-margin-top: 120px;
}

.section-label {
  font-size: 14px;
  font-weight: 600;
  color: #999999;
  text-transform: uppercase;
  letter-spacing: 1px;
  margin-bottom: 16px;
}

.section-title {
  font-size: 48px;
  font-weight: 800;
  color: #000000;
  margin: 0 0 40px 0;
  line-height: 1.1;
}

.section-content {
  font-size: 18px;
  line-height: 1.7;
  color: #333333;
}

.section-content p {
  margin: 0 0 24px 0;
}

.section-content p:last-child {
  margin-bottom: 0;
}

/* Responsive Design */
@media (max-width: 1200px) {
  .content-layout {
    gap: 60px;
    padding: 0 30px;
  }
  
  .sidebar {
    width: 280px;
  }
}

@media (max-width: 1024px) {
  .phone-mockups {
    flex-direction: column;
    align-items: center;
    gap: 60px;
  }
  
  .phone-left,
  .phone-right {
    transform: none;
  }
  
  .phone-mockup {
    width: 250px;
    height: 500px;
  }
  
  .content-layout {
    flex-direction: column;
    gap: 40px;
  }
  
  .sidebar {
    width: 100%;
    position: static;
    order: 2;
  }
  
  .sidebar-nav {
    flex-direction: row;
    flex-wrap: wrap;
    gap: 12px;
  }
  
  .sidebar-item {
    padding: 8px 12px;
    border-radius: 20px;
    background-color: #f8f8f8;
  }
  
  .sidebar-item.active {
    background-color: #000000;
    color: #ffffff;
  }
  
  .main-content {
    order: 1;
  }
}

@media (max-width: 768px) {
  .case-study-title {
    font-size: 48px;
  }
  
  .case-study-subtitle {
    font-size: 18px;
  }
  
  .nav-container {
    gap: 30px;
    padding: 15px 25px;
  }
  
  .nav-link {
    font-size: 14px;
  }
  
  .phone-mockup {
    width: 220px;
    height: 440px;
  }
  
  .transaction-amount {
    font-size: 28px;
  }
  
  .current-balance {
    font-size: 28px;
  }
  
  .content-layout {
    padding: 0 20px;
  }
  
  .section-title {
    font-size: 36px;
  }
  
  .section-content {
    font-size: 16px;
  }
}

@media (max-width: 480px) {
  .case-study-title {
    font-size: 36px;
  }
  
  .case-study-subtitle {
    font-size: 16px;
  }
  
  .nav-container {
    gap: 20px;
    padding: 12px 20px;
  }
  
  .nav-link {
    font-size: 13px;
    padding: 6px 12px;
  }
  
  .phone-mockup {
    width: 200px;
    height: 400px;
  }
  
  .section-title {
    font-size: 28px;
  }
  
  .section-content {
    font-size: 15px;
  }
}
