/* Goals Container */
.goals-container {
  display: flex;
  gap: 60px;
  margin-top: 40px;
}

.goal {
  flex: 1;
}

.goal-number {
  font-size: 72px;
  font-weight: 900;
  color: #000000;
  margin: 0 0 16px 0;
  line-height: 1;
}

.goal-title {
  font-size: 28px;
  font-weight: 700;
  color: #000000;
  margin: 0 0 16px 0;
  line-height: 1.2;
}

.goal-description {
  font-size: 18px;
  line-height: 1.6;
  color: #666666;
  margin: 0;
}

/* Team Photos */
.team-photos {
  margin-top: 40px;
  display: flex;
  flex-direction: column;
  gap: 16px;
}

.team-row {
  display: flex;
  gap: 16px;
  justify-content: center;
}

.team-member {
  flex-shrink: 0;
}

.team-photo {
  width: 120px;
  height: 120px;
  border-radius: 20px;
  background-color: #e0e0e0;
  background-image: linear-gradient(135deg, #f0f0f0 0%, #d0d0d0 100%);
}

/* Team Links */
.team-link {
  color: #000000;
  text-decoration: underline;
  font-weight: 600;
  transition: color 0.3s ease;
}

.team-link:hover {
  color: #666666;
}

/* Business Issues */
.business-issues {
  display: flex;
  gap: 60px;
  margin-top: 40px;
}

.issue {
  flex: 1;
}

.issue-number {
  font-size: 72px;
  font-weight: 900;
  color: #000000;
  margin: 0 0 16px 0;
  line-height: 1;
}

.issue-title {
  font-size: 28px;
  font-weight: 700;
  color: #000000;
  margin: 0 0 16px 0;
  line-height: 1.2;
}

.issue-description {
  font-size: 18px;
  line-height: 1.6;
  color: #666666;
  margin: 0;
}

/* Responsive Design */
@media (max-width: 768px) {
  .goals-container,
  .business-issues {
    flex-direction: column;
    gap: 40px;
  }
  
  .team-row {
    flex-wrap: wrap;
    justify-content: center;
  }
  
  .team-photo {
    width: 100px;
    height: 100px;
  }
  
  .goal-number,
  .issue-number {
    font-size: 60px;
  }
  
  .goal-title,
  .issue-title {
    font-size: 24px;
  }
}
